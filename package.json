{
  "name": "andela-qq",
  "version": "1.0.0",
  "description": "Quick Credit is an online lending platform that provides short term soft loans to individuals.",
  "type": "module",
  "main": "server.js",
  "dependencies": {
    "@babel/register": "^7.4.4",
    "@hapi/joi": "^15.0.0",
    "airbnb-browser-shims": "^3.2.1",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "should": "^13.2.3",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-preset-airbnb": "^3.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "build": "nyc mocha --exit --compilers js:babel-core/register server/test/**/*.spec.js",
    "start": "babel-node server.js",
    "test": "nyc mocha --require @babel/register --require babel-polyfill server/test/**/*.spec.js",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov --require babel-core/register  _mocha --exit ../test/*.spec.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info",
    "start:dev": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielufeli/andela-qq.git"
  },
